---
import Base from "./Base.astro";

interface Props {
    title?: string;
    description?: string;
    keywords: string[];
    defaultKeywords?: string[];
}

const { title, description, keywords, defaultKeywords } = Astro.props;
const correctTitle = title ? `${title} - ` : "";
const currentTitle = `${correctTitle}lab | Its-Just-Nans (aka n4n5)`;
---

<Base title={currentTitle} description={description} keywords={keywords} defaultKeywords={defaultKeywords}>
    <slot />
</Base>
